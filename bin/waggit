#!/usr/bin/env ruby

require 'waggit.rb'

cmd = ARGV[0].downcase

for arg in ARGV
  puts "arg: #{arg}"
end

options = []
if ARGV.length > 1
  options = ARGV[1..(ARGV.length-1)]
end


case cmd
when "s", "sync"
  Waggit.sync(options)
when "f", "forcepush", "fp"
  Waggit.forcepush(options)
else
  puts "Invalid command!"
end
